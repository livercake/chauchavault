{{#each addresses}}
    <tr data-address="{{@key}}">
        <td>{{@key}}</td>
        <td>0</td>
        <td>
            <button class="btn btn-success viewkey-btn">{{i18n "View Private Key"}}</button>
            <!-- <button class="btn btn-secondary signkey-btn">Sign</button> -->
            <button class="btn btn-secondary explorer-btn">{{i18n "Explorer"}}</button>
            <button class="btn btn-secondary qr-btn">{{i18n "QR"}}</button>
        </td>
        <td><button class="btn btn-danger deleteaddr-btn">{{i18n "Delete"}}</button></td>
    </tr>
{{/each}}
